{
  "name": "rust-crate-installer",
  "displayName": "Rust Crate Installer",
  "description": "Provides easy interface for interacting with crates.io",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rust-crate-installer.cargoAdd",
        "title": "Rust: Add Crate"
      },
      {
        "command": "rust-crate-installer.setTarget",
        "title": "Rust: Set Target"
      },
      {
        "command": "rust-crate-installer.newBin",
        "title": "Rust: New Application Project"
      },
      {
        "command": "rust-crate-installer.newLib",
        "title": "Rust: New Library Project"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "toml": "^3.0.0"
  }
}
